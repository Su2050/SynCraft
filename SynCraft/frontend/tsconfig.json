{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["DOM", "DOM.Iterable", "ES2020"],
      "jsx": "react-jsx",
      "moduleResolution": "Node",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["vite/client", "vitest/globals"]
    },
    "include": ["src"]
  }
  